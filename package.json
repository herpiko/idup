{
  "name": "idup",
  "preferGlobal": "true",
  "version": "0.0.1",
  "engines" : {
    "node" : ">=0.8"
  },
  "homepage" : "https://github.com/mdamt/idup",
  "description": "Modern CLI process manager for Node apps with a builtin load-balancer",
  "main": "index.js",
  "scripts": {
    "test": "bash ./test/cli.sh && bash ./test/reload.sh && bash ./test/gracefulReload.sh && bash ./test/cli2.sh && bash ./test/misc.sh && bash ./test/fork.sh && bash ./test/infinite_loop.sh && NODE_ENV=test ./node_modules/mocha/bin/mocha test"
  },
  "keywords": [
    "cli",
    "fault tolerant",
    "sysadmin",
    "tools",
    "idup",
    "monitoring",
    "process manager",
    "forever",
    "keep process alive",
    "process configuration",
    "clustering",
    "cluster cli",
    "cluster",
    "cron",
    "devops",
    "dev ops"
  ],
  "bin": {
    "idup": "./bin/idup"
  },
  "dependencies": {
    "commander"      : "2.8.1",
    "cli-table"      : "0.2.0",
    "pm2-multimeter" : "0.1.2",
    "pm2-interface"  : "0.1.0",
    "usage"          : "0.3.9",
    "axon-rpc"       : "0.0.2",
    "watch"          : "0.8.0",
    "axon"           : "1.0.0",
    "cron"           : "1.0.1",
    "colors"         : "0.6.2",
    "coffee-script"  : "*",
    "eventemitter2"  : "0.4.13",
    "debug"          : "*",
    "async"          : "0.2.9"
  },
  "devDependencies": {
    "mocha"         : "1.x",
    "should"        : "1.x",
    "better-assert" : "*"
  },
  "repository": {
    "type" : "git",
    "url" : "git://github.com/mdamt/idup.git"
  },
  "license": "Apache v2"
}
